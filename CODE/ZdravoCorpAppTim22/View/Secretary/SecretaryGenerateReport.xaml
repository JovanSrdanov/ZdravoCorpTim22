<Window x:Class="ZdravoCorpAppTim22.View.Secretary.SecretaryGenerateReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorpAppTim22.View.Secretary"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="SecretaryGenerateReport" Height="750" Width="430" Closed="Window_Closed">
    <Grid>
        <DatePicker x:Name="dateStart" HorizontalAlignment="Left" Margin="230,24,0,0" VerticalAlignment="Top" Width="182"/>
        <DatePicker x:Name="DateEnd" HorizontalAlignment="Left" Margin="230,69,0,0" VerticalAlignment="Top" Width="182"/>
        <Label x:Name="label" Content="Start date:" HorizontalAlignment="Left" Margin="10,14,0,0" VerticalAlignment="Top" FontSize="20" FontWeight="Bold"/>
        <Label x:Name="label_Copy" Content="End date:" HorizontalAlignment="Left" Margin="10,59,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.453,2.192" FontSize="20" FontWeight="Bold"/>
        <Button x:Name="button" Content="Generate report" HorizontalAlignment="Left" Margin="10,613,0,0" VerticalAlignment="Top" Width="402" Height="96" FontWeight="Bold" FontSize="40" Click="button_Click"/>

    </Grid>
</Window>
