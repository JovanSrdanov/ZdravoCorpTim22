<Window x:Class="ZdravoCorpAppTim22.View.DoctorView.DoctorHome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorpAppTim22.View.DoctorView"
        mc:Ignorable="d"
        Title="DoctorHome" Height="768" Width="1024" Closed="DoctorHomeClose">
    <Grid>
        <ComboBox x:Name="SelectDoctorCBOX" HorizontalAlignment="Left" Margin="360,244,0,0" VerticalAlignment="Top" Width="249" DisplayMemberPath="Name"
                  SelectedIndex="0" FontSize="35">
        </ComboBox>
        <Label Content="Select a doctor" HorizontalAlignment="Left" Margin="381,120,0,0" VerticalAlignment="Top" FontSize="30"/>
        <Button x:Name="NextButton" Content="Next" HorizontalAlignment="Left" Margin="620,561,0,0" VerticalAlignment="Top" 
                RenderTransformOrigin="-0.553,0.607" Width="58" Click="NextButton_Click" Foreground="White" Background="MediumPurple" FontSize="25"/>
        <Button x:Name="BackBtn" Content="Back" HorizontalAlignment="Left" Margin="271,561,0,0" VerticalAlignment="Top" 
                RenderTransformOrigin="0.174,-0.131" Width="120" Click="BackBtnClick" Foreground="White" Background="MediumPurple" FontSize="25"/>

    </Grid>
</Window>
