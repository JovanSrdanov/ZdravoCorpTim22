<Window x:Class="ZdravoCorpAppTim22.View.DoctorView.DoctorHome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorpAppTim22.View.DoctorView"
        mc:Ignorable="d"
        Title="DoctorHome" Height="450" Width="800" Closed="DoctorHomeClose">
    <Grid>
        <ComboBox x:Name="SelectDoctorCBOX" HorizontalAlignment="Left" Margin="317,122,0,0" VerticalAlignment="Top" Width="120" DisplayMemberPath="Name"
                  SelectedIndex="0">
        </ComboBox>
        <Label Content="Select a doctor" HorizontalAlignment="Left" Margin="332,67,0,0" VerticalAlignment="Top"/>
        <Button x:Name="NextButton" Content="Next" HorizontalAlignment="Left" Margin="438,301,0,0" VerticalAlignment="Top" 
                RenderTransformOrigin="-0.553,0.607" Width="58" Click="NextButton_Click" Foreground="White" Background="MediumPurple"/>
        <Button x:Name="BackBtn" Content="Back" HorizontalAlignment="Left" Margin="262,301,0,0" VerticalAlignment="Top" 
                RenderTransformOrigin="0.174,-0.131" Width="55" Click="BackBtnClick" Foreground="White" Background="MediumPurple"/>

    </Grid>
</Window>
