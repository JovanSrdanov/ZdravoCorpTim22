<Window x:Class="ZdravoCorpAppTim22.View.DoctorView.DoctorHomeScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorpAppTim22.View.DoctorView"
        mc:Ignorable="d"
        Title="DoctorHomeScreen" Height="768" Width="1024" Closed="DoctorHomeScreenClose">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="Welcome to Zdravo Corp"
                   Foreground="MediumPurple" FontWeight="Bold" FontSize="40" RenderTransformOrigin="0.508,0.28"/>
        <Button x:Name="SchedulingBtn" Content="Scheduling list" HorizontalAlignment="Left" Margin="138,269,0,0" VerticalAlignment="Top" 
                Foreground="White" Background="MediumPurple" Click="ScheduleListBtn" FontSize="25"/>
        <Button x:Name="SchedulingBtn_Copy" Content="View medical records" HorizontalAlignment="Left" Margin="602,442,0,0" VerticalAlignment="Top"
                Foreground="White" Background="MediumPurple" Click="ViewMedRecordsClick" FontSize="20"/>
        <Button x:Name="LogOut" Content="logout" HorizontalAlignment="Left" Margin="933,37,0,0" VerticalAlignment="Top" Click="LogOutBtn"
                Foreground="White" Background="MediumPurple" FontSize="20"/>

    </Grid>
</Window>
